    "import re\n",
    "def insert_space_between_numbers(text):\n",
    "    pattern = re.compile(r'(\\d)([A-Z])')\n",
    "    result = pattern.sub(r'\\1 \\2',text)\n",
    "    return result\n",
    "text = \"RegularExpression1IsAn2ImportantTopic3InPython\"\n",
    "result = insert_space_between_numbers(text)\n",
    "print(result)"
