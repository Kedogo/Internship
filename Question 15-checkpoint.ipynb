   "source": [
    "import re\n",
    "\n",
    "text = \"The quick brown fox jumps over the lazy dog.\"\n",
    "searched_words = ['fox', 'dog','horse']\n",
    "\n",
    "for word in searched_words:\n",
    "    if re.search(word, text):\n",
    "        print(f\"'{word}' found\")\n",
    "    else:\n",
    "        print(f\"'{word}' not found\")"
   ]
